{
    "name": "mkkvkmn",
    "dataColors": [
        "#d4cfc6",
        "#5543db",
        "#5a5963",
        "#0A0D15",
        "#33CC33",
        "#CC3333"
    ],
    "visualStyles": {
        "page": {
            "*": {
                "background": [
                    {
                        "color": {
                            "solid": {
                                "color": "#0A0D15"
                            }
                        },
                        "transparency": 0
                    }
                ],
                "outspace": [
                    {
                        "color": {
                            "solid": {
                                "color": "#0A0D15"
                            }
                        },
                        "transparency": 0
                    }
                ]
            }
        },
        "card": {
			"*": {
				"labels": [{
					"color": { "solid": { "color": "#FFFFFF"}},
					"labelPrecision": 0,
					"fontSize": 30,
                    "bold": true
				}],
                "title": [{
				    "show": false,
				    "alignment": "center"
				}],
                "categoryLabels": [{
					"color": { "solid": { "color": "#FFFFFF"}},
					"show": true
				}],
                "border": [{
                    "show": true,
                    "color": { "solid": { "color": "#FFFFFF"}}
                }]
			}
        },
        "slicer": {
			"*": {
				"general": [{
					"outlineWeight": 0,
					"responsive": false
				}],
				"background": [{
					"show": true,
                    "color": { "solid": { "color": "#0A0D15"}}
				}],
                "border": [{
                    "show": false
                }],
                "padding": [{
                        "top": 12,
                        "bottom": 0,
                        "left": 5,
                        "right": 5
                }],
				"data": [{
					"mode": "Dropdown"
				}],
				"selection": [{
					"selectAllCheckboxEnabled": true,
					"singleSelect": false
				}],
				"header": [{
					"show": true,
					"fontColor": { "solid": { "color": "#FFFFFF"}},
					"outline": "None",
                    "textSize": 12

				}],
				"items": [{
					"fontColor": { "solid": { "color": "#FFFFFF"}},
					"outline": "None",
                    "background": { "solid": { "color": "#0A0D15"}},
                    "textSize": 10
				}],
				"slider": [{ 
					"color": {"solid": { "color": "#FFFFFF"} }
				}],
                "title": [{
                    "show": false
                }]
			}
		},
        "shape": {
			"*": {
                "line": [{
					"lineColor": { "solid": { "color": "#FFFFFF"}},
					"transparency": 0,
					"weight": 1,
					"roundEdge": 3
				}],
				"fill": [{
					"show": false
				}],
				"rotation": [{
					"angle": 0
				}],
				"visualHeader": [{
					"show": false
				}],
                "outline": [{
                    "show": false
                }],
                "title": [{
                    "show": false
                }]
			}
		},
        "pivotTable": {
			"*": {
				"stylePreset":[{"name":"mkkvkmn"}],
                "columnHeaders": [{
					"backColor": { "solid": { "color": "#0A0D15"}},
					"fontColor": { "solid": { "color": "#FFFFFF"}},
					"autoSizeColumnWidth": false,
					"fontSize": 10,
                    "bold": true,
					"alignment": "Left",
					"wordWrap": true
                    }],
                "rowHeaders": [{
					"fontSize": 10,
					"fontColor": { "solid": { "color": "#FFFFFF"}},
					"backColor": { "solid": { "color": "#0A0D15"}},
					"wordWrap": false,
					"alignment": "Left", 
					"showExpandCollapseButtons":true
				}],
				"grid": [{
					"gridVertical": false,
					"gridHorizontal": false,
					"rowPadding": 1,
                    "outline":"BottomOnly",
					"outlineWeight": 1,
                    "outlineColor": {"solid": {"color": "#FFFFFF"}}
				}],
                "values": [{
                    "fontColorPrimary": { "solid": { "color": "#FFFFFF"}},
					"backColorPrimary": { "solid": { "color": "#0A0D15"}},
					"fontColorSecondary": { "solid": { "color": "#B3B3B3"}},
					"backColorSecondary": { "solid": { "color": "#0A0D15"}},
					"outline": "None",
					"urlIcon": true,
					"wordWrap": false,
					"fontSize": 10
				}],
                "total": [{
					"totals": true,
					"bold": true
				}]
			}
		},
        "tableEx": {
			"*": {
				"stylePreset":[{"name":"mkkvkmn"}],
                "columnHeaders": [{
                    "fontColor": { "solid": { "color": "#FFFFFF"}},
					"backColor": { "solid": { "color": "#0A0D15"}},
					"autoSizeColumnWidth": false,
					"fontSize": 10,
                    "bold": true,
					"alignment": "Left",
					"wordWrap": true
				}],
				"grid": [{
					"gridVertical": false,
					"gridHorizontal": false,
					"rowPadding": 1,
                    "outline":"BottomOnly",
					"outlineWeight": 1,
                    "outlineColor": {"solid": {"color": "#FFFFFF"}}
				}],
                "values": [{
                    "fontColorPrimary": { "solid": { "color": "#FFFFFF"}},
					"backColorPrimary": { "solid": { "color": "#0A0D15"}},
					"fontColorSecondary": { "solid": { "color": "#696969"}},
					"backColorSecondary": { "solid": { "color": "#0A0D15"}},
					"outline": "None",
					"urlIcon": true,
					"wordWrap": false,
					"fontSize": 10
				}],
                "total": [{
					"totals": true,
					"bold": true
				}]
			}
		},
        "*": {
            "*": {
                "background": [{
					"show": true,
                    "color": { "solid": { "color": "#0A0D15"}}
				}],
                "border": [{
                    "show": true,
                    "color": { "solid": { "color": "#0A0D15" } },
                    "radius": 3
                }],
                "padding": [{
                    "top": 15,
                    "bottom": 15,
                    "left": 15,
                    "right": 15
                }],
                "visualHeader": [
                    {
                        "background": {
                            "solid": {
                                "color": "#FFFFFF"
                            }
                        },
                        "foreground": {
                            "solid": {
                                "color": "#696969"
                            }
                        },
                        "transparency": 0
                    }
                ],
                "visualTooltip": [
                    {
                        "valueFontColor": {
                            "solid": {
                                "color": "#FFFFFF"
                            }
                        }
                    }
                ],
                "visualHeaderTooltip": [
                    {}
                ],
                "categoryAxis": [{
                    "labelColor": { "solid": { "color": "#B3B3B3" }},
					"showAxisTitle": false
				}],
				"valueAxis": [{
                    "labelColor": { "solid": { "color": "#B3B3B3" }},
					"showAxisTitle": false
				}],
				"y2Axis": [{
					"show": false
				}],  
                "title": [{
                    "show": true,
                    "fontColor": {"solid": { "color": "#FFFFFF" }},
                    "fontSize": 14
                }],
                "labels": [{
                    "show": true,
                    "enableBackground": false,
                    "color": {"solid": { "color": "#FFFFFF" }}
                }]     
            }
        }
    },
    "textClasses": {
        "label": {
            "color": "#FFFFFF",
            "fontSize": 12
        },
        "lightLabel": {
            "color": "#FFFFFF"
        },
        "callout": {
            "color": "#FFFFFF"
        },
        "title": {
            "color": "#FFFFFF",
            "fontSize": 14
        },
        "header": {
            "color": "#FFFFFF"
        }
    },
    "foreground": "#000000"
}